package loopsexample;

public class WhileLoopExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 for (int i=1;i<3;i++){
			 for(int j=1;j<=2;j++) {
				 System.out.println("****");
				 
				 
			 }
		 }
			 System.out.println();
			 System.out.println("aaaaaaaaaaaa");
			 System.out.println("11111111");
			 
			 
			 
			 
			 
			 
			 

}

	
 
 

	

		 
		 
		 
		 

	}


